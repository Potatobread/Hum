# include <stdio.h>
# define L 4
# define C 4

int main () {
	int i,j,x,y,mat[L][C],soma_a=0,soma_b=0,soma_c=0,soma_d=0;
	
	for(j=0;j<L;j++){
		for(i=0;i<C;i++)
			scanf(" %d", &mat[j][i]);
	}
	
	for(j=0;j<L;j++)
		soma_a = soma_a + mat[j][j];   	
	
	x = 2;
	for(j=0;j<L;j++){
		soma_b = soma_b + mat[x][j];
		x--;
	}	
	
	x = 0;
	for(j=0;j<L;j++){
		for(i=0;i<C;i++){
			soma_c = soma_c + mat[j][x];
			x++;
		}
	}
	
	for(j=1;j<=L;j++){
		y = 0;
		for(i=0;i==j;i++){	
			soma_d = soma_d + mat[j][y];
			y++;
		}
	}	
	
	printf("%d\n", soma_a);
	printf("%d\n", soma_b);
	printf("%d\n", soma_c);	
	printf("%d\n", soma_d);
		
	return 0;
}
