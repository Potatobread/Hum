#include <stdio.h>

int fibonacci(int n){
	int i,valor,fib[61];
	fib[0] = 0, fib[1] = 1;
	
	for(i=2;i<=60;i++){
		fib[i] = fib[i-1] + fib[i-2];
	}
	
	valor = fib[n];
	return valor;
}

int main(){
	int t,n,x;
	
	scanf("%d", &t);
	while(t != 0){
		scanf("%d", &n);
		x = fibonacci(n);		
		printf("Fib(%d) = %d\n", n,x);
		t--;
	}
	
	return 0;
}
