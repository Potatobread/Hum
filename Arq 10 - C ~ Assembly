#include <stdio.h>

int main(){
    
    int numero = 85, soma = 0;


    while(numero < 907){
    	if(numero % 2 == 0){
    		printf("Numero: %d\n", numero);
    		soma = soma + numero;
		}	
    	
    	numero++;
	}
	
	printf("Soma: %d\n", soma);
    
    return 0;
}


.data
	numero: .asciiz "\nNumero: "
	soma: .asciiz "\nSoma: "
	
.text
	li $t0, 85
	move $t1, $zero		# soma = 0
		
	while:
		li $t2,2
		div $t0, $t2
		mfhi $s1
		beq $s1, $zero, if
		
		addi $t0, $t0, 1
		
		j fim
		
		if:
			li $v0, 4
			la $a0, numero
			syscall 
			
			li $v0, 1
			move $a0, $t0
			syscall
			
			add $t1, $t1, $t0
			
			addi $t0, $t0, 1
		fim:
		
		slti $t3, $t0, 907
		beq $t3, $zero, final
		
		j while	
		
		final: 				
			li $v0, 4
			la $a0, soma
			syscall 
			
			li $v0, 1
			move $a0, $t1
			syscall
