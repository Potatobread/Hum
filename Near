/*
nome da conta e o valor, N vezes /*
*/
#include<stdio.h>
#include<stdlib.h>

typedef struct{
	char nome[20];
	int valor;
}conta;

typedef struct No{
 	conta info[3];
    struct No *ant;
    struct No *prox;
}No;

void crialistaLDE(No *l){
    No *NovaLista
	*NovaLista = alocanoLDE(l);
	if(NovaLista == 1){
		printf("Try again, but harder this time");
		exit(1);		
	}
	novaLista->ant = NULL;
    l->prox = NULL;
    return l;
}


No* alocano_LDE(){
    No *novo;
    novo = (No *)malloc(sizeof(No));
    if(novo == NULL){
        printf("\nMEMORIA INSUFICIENTE!\n");
        exit(1);
    }
    return novo;
}


void inserirPosicao_LDE(No *l, int k,conta bills){
    if(k < 1){
        printf("Posicao invalida!");
        return;
    }else{
        int i = 1;
        No *aux = l->prox;
        No *ant = l;
        while(i < k && aux != NULL){
            ant = aux;
            aux = aux->prox;
            i++;
        }
        if(k > i + 1)
        {
            printf("posicao invalida!");
            return;
        }else{
            inserirdepois_LDE(l, ant, info);
		}
    }
}




int main(){
	No listaDuplEncadeada;
	crialistaLDE(&listaDuplEncadeada);
}
