# include <stdio.h>
# define V 5

int main (){
	int vet[V],i,j,remove,ultimo,cont;
	
	for(i=0;i<V;i++){
		printf("Digite o valor %d: ", i+1);
		scanf(" %d", &vet[i]);
	}
	printf("\n");
	
	printf("Remover codigo igual a: ");
	scanf(" %d",&remove);
	
	ultimo = V;

	for(i=0;i<ultimo;i++){
		if(vet[i] == remove){
        	for(j=i;j<ultimo-1;j++){
            	vet[j] = vet[j+1];
			}
        	ultimo--;
        	cont = i;
        }
    	printf("codigo = %d\n",vet[i]);
    }
    
    printf("%d\n", vet[cont]);
    
    return 0;
}
