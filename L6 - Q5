# include <stdio.h>
# define P 3
# define A 3

int main(){
	int arm[A][P],valor[P],i,j,valor_total=0,t1,t2,t3;

	
	for(i=0; i<A; i++){
		for(j=0; j<P; j++){
			printf("Digite a quantidade do produto %d do Armazem %d: ", j+1,i+1);
			scanf(" %d", &arm[i][j]);
		}
		printf("\n");
	}
	
	for(i=0; i<P; i++){
		printf("Digite o valor do produto %d: ", i+1);
		scanf(" %d", &valor[i]);
	}	
	
	for(i=0; i<A; i++){
		for(j=0; j<P; j++){
			printf("Total do produto %d no Armazem %d: %d\n", j+1,i+1,arm[i][j]);
		}
		printf("\n");
	}
	
	return 0;
}
