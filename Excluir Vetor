#include <stdio.h>

int main(){
	int v[5];
	int i,rem;
	
	for(i=0;i<5;i++){
		scanf("%d", &v[i]);
	}
	
	for(i=0;i<5;i++){
		printf("%d ", v[i]);
	}
	printf("\n");
	
	printf("Remover qual valor?: ");
	scanf("%d", &rem);
	for(i=0;i<5;i++){
		if(v[i]==rem){
			v[i]=v[i+1];
			v[5-1]=0;			
		}
		printf("%d ", v[i]);
	}
	
	return 0;
}
