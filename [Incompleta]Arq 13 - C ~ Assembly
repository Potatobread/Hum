#include <stdio.h>

int main(){

	int n1, n2, maior, menor, resultado = 0;
	
	printf("Digite o primeiro numero : ");
	scanf("%d", &n1);
	printf("Digite o segundo numero : ");
	scanf("%d", &n2);
	
	if(n1 < n2){
		maior = n2;
		menor = n1;
	} else{
		maior = n1;
		menor = n2;
	}
	
	while(menor > 0){
		resultado = resultado + maior;
		menor--;
	}
		 
	printf("Resultado: %d\n", resultado);
	
	return 0;
}

/*

Sem utilizar a operação de multiplicação, escreva um algoritmo que multiplique dois números 
inteiros (positivos). Ler os dois números e imprimir o resultado na forma: 
Por exemplo: 5 * 3 = 15

*/


.data
	primeiro: .asciiz "Digite o primeiro numero: "
	segundo: .asciiz "Digite o segundo numero: "
	resultado: .asciiz "\nResultado: "
	
.text
	move $t4, $zero		# resultado = 0
	
	li $v0, 4
	la $a0, primeiro
	syscall
	
	li $v0, 5
	syscall
	move $t0, $v0
	
	li $v0, 4
	la $a0, segundo
	syscall
	
	li $v0, 5
	syscall
	move $t1, $v0
	
	slt $t5, $t0, $t1
	bne $t5, $zero, if
	
	move $t2, $t0
	move $t3, $t1
	
	j fim1
	
	if:
		move $t2, $t1
		move $t3, $t0
	
	fim1
	
	while:
		add $t4, $t4, $t2
		subi $t3, $t3, 1
	
	slt $t5, $t3, $zero
	beq $t5, $zero, 
