#include <stdio.h>

int primo(int n){
	int i,cont=0;
	
	for(i=n;i==0;i--){
		if(n % i == 0){
			cont++;
		}
		if(cont == 2){
			return 1
		}
		else{
			return 0;
		}
	}
}

int main(){
	int n,cont=0,i,resul;
	
	printf("Entre com um numero: ");
	scanf("%d", &n);
	while(n != -1){
		if(n < 0){
			printf("O numero %d deve ser desprezado\n");
		}
		else{
			resul = primo(n);
		}
		if(resul == 1){
			
		}
	}
	
	return 0;
}
